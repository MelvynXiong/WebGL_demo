{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "jsx": "react",
    "moduleResolution": "node",
    "strict": true,
    "target": "es5",
    "lib": ["esnext", "dom"],
    "sourceMap": true,
    "baseUrl": ".",
    "paths": { "@/*": ["./src/*"] }
  },
  "exclude": [
    "node_modules",
    "config",
  ]
}
